<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="pnlSettingsTime">
		<size>600,400</size>
		<bg>#000000</bg>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wmButton" name="ID_M_PBTN22" variable="m_pbtnPTP" member="yes">
			<label>PTP</label>
			<toggle_look>1</toggle_look>
			<toggle_left>Off</toggle_left>
			<toggle_right>Sync</toggle_right>
			<toggle_percent>50</toggle_percent>
			<pos>10,10</pos>
			<size>200,40</size>
			<fg>#FFFFFF</fg>
			<bg>#008000</bg>
			<style>wmButton::STYLE_SELECT</style>
			<handler function="OnbtnPTPClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmButton" name="ID_M_PBTN1" variable="m_pbtnLTC" member="yes">
			<label>LTC</label>
			<toggle_look>1</toggle_look>
			<toggle_left>Off</toggle_left>
			<toggle_right>Sync</toggle_right>
			<toggle_percent>50</toggle_percent>
			<pos>240,10</pos>
			<size>200,40</size>
			<fg>#FFFFFF</fg>
			<bg>#008000</bg>
			<style>wmButton::STYLE_SELECT</style>
			<handler function="OnbtnLTCClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmButton" name="ID_M_PBTN2" variable="m_pbtnNTP" member="yes">
			<label>NTP</label>
			<toggle_look>1</toggle_look>
			<toggle_left>Off</toggle_left>
			<toggle_right>Sync</toggle_right>
			<toggle_percent>50</toggle_percent>
			<pos>10,60</pos>
			<size>200,40</size>
			<fg>#FFFFFF</fg>
			<bg>#008000</bg>
			<style>wmButton::STYLE_SELECT</style>
			<handler function="OnbtnNTPClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmLabel" name="ID_M_PLBL3" variable="m_pLbl3" member="yes">
			<label>NTP Servers</label>
			<pos>0,110</pos>
			<size>600,30</size>
			<fg>#FFFFFF</fg>
			<bg>#004000</bg>
			<font>
				<size>10</size>
				<style>normal</style>
				<weight>bold</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Arial</face>
			</font>
		</object>
		<object class="wmList" name="ID_M_PLST1" variable="m_plstNTPServers" member="yes">
			<scrolling>None</scrolling>
			<button_gap>5,5</button_gap>
			<button_size>-1,40</button_size>
			<columns>3</columns>
			<button_selected>#008000</button_selected>
			<button_disabled>#808080</button_disabled>
			<items />
			<pos>0,140</pos>
			<size>600,250</size>
			<bg>#000000</bg>
			<style>wmList::STYLE_SELECT</style>
			<handler function="OnlstNTPServersSelected" entry="EVT_LIST_SELECTED" />
		</object>
		<object class="wmButton" name="ID_M_PBTN3" variable="m_pbtnNtpServerAdd" member="yes">
			<label>Add Server</label>
			<button_disabled>#808080</button_disabled>
			<pos>70,400</pos>
			<size>100,40</size>
			<handler function="OnbtnNtpServerAddClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmButton" name="ID_M_PBTN4" variable="m_pbtnNtpServerEdit" member="yes">
			<label>Edit Server</label>
			<button_disabled>#808080</button_disabled>
			<pos>180,400</pos>
			<size>100,40</size>
			<enabled>0</enabled>
			<handler function="OnbtnNtpServerEditClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmButton" name="ID_M_PBTN5" variable="m_pbtnNtpServerDelete" member="yes">
			<label>Delete Server</label>
			<button_disabled>#808080</button_disabled>
			<pos>290,400</pos>
			<size>100,40</size>
			<enabled>0</enabled>
			<handler function="OnbtnNtpServerDeleteClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmButton" name="ID_M_PBTN6" variable="m_pbtnNTPServerDeleteAll" member="yes">
			<label>Delete All</label>
			<button_disabled>#808080</button_disabled>
			<pos>400,400</pos>
			<size>100,40</size>
			<handler function="OnbtnNTPServerDeleteAllClick" entry="EVT_BUTTON" />
		</object>
	</object>
</wxsmith>
